{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "prettier"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["warn"],
    "react/prop-types": 0,
    "react/no-unescaped-entities": 0,
    "react/jsx-key": 1,
    "no-unused-vars": 1
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
